{
  "remote.SSH.remotePlatform": {
    "dragon": "linux"
  },
  "remote.SSH.configFile": "~/.ssh/config",
  "files.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/.expo": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true
  }
}
